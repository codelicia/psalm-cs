{
  "name": "codelicia/psalm-cs",
  "description": "A CodeSniffer sniff to fix array shapes",
  "type": "phpcodesniffer-standard",
  "require-dev": {
    "phpunit/phpunit": "10.3.x",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.0@dev",
    "staabm/annotate-pull-request-from-checkstyle": "^1.8.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jefersson Nathan",
      "email": "malukenho.dev@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "~8.1.0 | ~8.2.0 | ~8.3.0",
    "azjezz/psl": "^2.3.0",
    "slevomat/coding-standard": "8.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Codelicia\\Psalm\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
